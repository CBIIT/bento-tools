{
  "name": "bento-components",
  "version": "0.1.11",
  "private": false,
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "peerDependencies": {
    "react": "^16.8.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "cross-env": "^7.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-jss": "^10.1.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "svg-url-loader": "^5.0.0"
  },
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib --copy-files",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --copy-files",
    "build": "npm run build:commonjs && npm run build:es",
    "prepare": "npm run clean && npm run lint && npm run build",
    "storybook": "start-storybook -p 9001"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-create-react-app": "^2.1.2",
    "@storybook/react": "^5.3.18",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^6.0.0"
  }
